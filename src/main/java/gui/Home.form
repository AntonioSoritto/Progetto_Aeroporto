<?xml version="1.0" encoding="UTF-8"?>
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="gui.LoginForm">
  <grid id="12345" binding="LoginForm" layout-manager="GridBagLayout">
    <constraints>
      <xy x="15" y="20" width="400" height="200"/>
    </constraints>
    <properties/>
    <border type="none"/>
    <children>
      <!-- Username Label -->
      <component id="labelUsername" class="javax.swing.JLabel">
        <constraints>
          <grid row="0" column="0"/>
          <gridbag weightx="0.0" weighty="0.0"/>
        </constraints>
        <properties>
          <text value="Username:"/>
        </properties>
      </component>
      
      <!-- Username Field -->
      <component id="fieldUsername" class="javax.swing.JTextField" binding="usernameField">
        <constraints>
          <grid row="0" column="1"/>
          <gridbag weightx="1.0" weighty="0.0" fill="1"/>
        </constraints>
        <properties/>
      </component>
      
      <!-- Password Label -->
      <component id="labelPassword" class="javax.swing.JLabel">
        <constraints>
          <grid row="1" column="0"/>
          <gridbag weightx="0.0" weighty="0.0"/>
        </constraints>
        <properties>
          <text value="Password:"/>
        </properties>
      </component>
      
      <!-- Password Field -->
      <component id="fieldPassword" class="javax.swing.JPasswordField" binding="passwordField">
        <constraints>
          <grid row="1" column="1"/>
          <gridbag weightx="1.0" weighty="0.0" fill="1"/>
        </constraints>
        <properties/>
      </component>
      
      <!-- Login Button -->
      <component id="buttonLogin" class="javax.swing.JButton" binding="loginButton">
        <constraints>
          <grid row="2" column="0" col-span="2"/>
          <gridbag weightx="0.0" weighty="0.0" anchor="10"/>
        </constraints>
        <properties>
          <text value="Login"/>
        </properties>
      </component>
    </children>
  </grid>
</form>
